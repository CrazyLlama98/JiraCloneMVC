@using JiraCloneMVC.Web.Models
@using JiraCloneMVC.Web.ViewModels
@model dynamic

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["projectId"] = Model.Project.Id;
}
<div class="container my-5">
    <h2>Project members</h2>

    @if (ViewBag.Rol.ToString().Equals("Organizator") || ViewBag.Rol.ToString().Equals("Administrator"))
    {
        @Html.Partial("DetailsMembersForOrganizerAndAdmin", ViewData);
    }
    else
    {
        @Html.Partial("DetailsMembersForMembers", ViewData);
    }
</div>